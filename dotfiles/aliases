alias cls="clear"
alias ls="ls -Ga"
alias ll="ls -latrG"

#tmux
alias ta="tmux attach"

#folders
alias configfiles="cd ~/workspace/configfiles"
alias workspace="cd ~/workspace"


# GIT
alias ci="git commit"
alias st="git status"
alias glog="git log"

# Ruby
alias bil="bundle install"
alias resc="rake spec"

#sublime text 2
alias s="subl -w"

#textmate
alias m="mate ."

#mysql
alias mysql=/usr/local/mysql/bin/mysql
alias mysqladmin=/usr/local/mysql/bin/mysqladmin
