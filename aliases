alias ll="ls -latrG"
alias st="git status"
alias ta="tmux attach"
alias cq="sudo iwconfig wlan0 essid CLUBQUARTERS && sudo dhcpcd wlan0"
alias mvnconsole='mvn -o compile dependency:build-classpath -Dmdep.outputFile=/var/tmp/.classpath && scala -cp `cat /var/tmp/.classpath`:./target/classes'
alias hdmi="echo DDIS > $HOME/switch && sudo /etc/rc.d/kdm restart"
alias t2t="cd ~/work/this2that/android"
